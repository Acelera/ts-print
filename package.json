{
  "name": "ts-print",
  "description": "Biblioteca útil para debug em node, com cores e datas formatadas, se propõe a ser intuitiva.",
  "version": "1.1.2",
  "license": "MIT",
  "author": "Marcus Yoda <marcus@acelera.cloud>",
  "repository": "https://github.com/acelera/ts-print.git",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "ci:dev": "yarn install --frozen-lockfile",
    "ci:prod": "yarn install --frozen-lockfile --production",
    "ci": "yarn ci:dev",
    "build": "rollup -c",
    "coverage": "nyc -r lcov -e .ts -x \"*.test.ts\" npm run test",
    "deploy": "yarn build && yarn publish",
    "install-lock": "yarn --frozen-lock",
    "lint": "cross-env NODE_OPTIONS=--max_old_space_size=3000 eslint src/**/*.ts",
    "lint:fix": "yarn lint -- --fix",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--max_old_space_size=2048 jest --passWithNoTests --no-cache --forceExit --detectOpenHandles",
    "test:unit": "npm test -- --watch --runInBand -c jest-unit-config.js --coverage",
    "test:staged": "yarn test -- --bail --findRelatedTests",
    "test:coverage": "yarn test -- -c jest-coverage-config.js",
    "test:ci": "npm test -- --runInBand --coverage"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^18.0.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cross-env": "^7.0.3",
    "eslint": "8.18.0",
    "husky": "^8.0.1",
    "jest": "^27.2.5",
    "jest-sonar": "^0.2.12",
    "prettier": "^2.3.2",
    "rollup": "^2.64.0",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {}
}
